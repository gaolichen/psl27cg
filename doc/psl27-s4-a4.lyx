\begin_layout Standard
Let 
\begin_inset Formula $H$
\end_inset

 be a subgroup of 
\begin_inset Formula $G$
\end_inset

.
 
\begin_inset Formula $X$
\end_inset

, 
\begin_inset Formula $Y$
\end_inset

, 
\begin_inset Formula $Z$
\end_inset

 are vectors of irreducible representations of 
\begin_inset Formula $G$
\end_inset

.
 
\begin_inset Formula 
\[
X=\bigoplus x_{i},\quad Y=\bigoplus y_{i},\quad Z=\bigoplus z_{i}.
\]

\end_inset

 Let us consider the CG coefficients 
\begin_inset Formula $X\otimes Y\to Z$
\end_inset

.
 In terms of representations of 
\begin_inset Formula $H$
\end_inset

, one can write 
\begin_inset Formula 
\[
Z=\bigoplus_{i}\sum_{j,k}c_{i,jk}\left\{ \left(P_{X\to j}X\right)\otimes\left(P_{Y\to k}Y\right)\right\} _{z_{i}}.
\]

\end_inset

 where 
\begin_inset Formula $P_{X\to j}$
\end_inset

 is the project matrix to obtain the 
\begin_inset Formula $x_{j}$
\end_inset

 representation of 
\begin_inset Formula $H$
\end_inset

 from 
\begin_inset Formula $X$
\end_inset

.
 
\begin_inset Formula 
\[
P_{X\to j}=\begin{pmatrix}O_{n_{1}}\big| & O_{n_{2}}\big| & \cdots\big| & I_{j}\big| & O_{n_{j+1}}\big|\cdots\end{pmatrix}
\]

\end_inset

 Let 
\begin_inset Formula $g\in G$
\end_inset

, 
\begin_inset Formula 
\[
R_{Z}\left(g\right)Z=\bigoplus_{i}\sum_{j,k}c_{i,jk}\left\{ \left(P_{X\to j}R_{X}\left(g\right)X\right)\otimes\left(P_{Y\to k}R_{Y}\left(g\right)Y\right)\right\} _{z_{i}}.
\]

\end_inset


\end_layout

\begin_layout Standard
Under the transform
\begin_inset Formula 
\[
x_{i}\to x_{i}^{\prime}=u_{i}x_{i},
\]

\end_inset

 the CG coefficients of the subgoup 
\begin_inset Formula $H$
\end_inset

 changes as
\begin_inset Formula 
\[
z_{i}=\left\{ x_{j}\otimes y_{k}\right\} _{z_{i}}\to\left\{ u_{j}x_{j}\otimes u_{k}y_{k}\right\} _{u_{i}z_{i}}^{\prime}=\left\{ \left(u_{j}\otimes u_{k}\right)\left(x_{j}\otimes y_{k}\right)\right\} _{u_{i}z_{i}}^{\prime}
\]

\end_inset


\begin_inset Formula 
\[
u_{i}z_{i}=\left\{ u_{j}x_{j}\otimes u_{k}y_{k}\right\} _{u_{i}z_{i}}^{\prime}=u_{i}\left\{ x_{j}\otimes y_{k}\right\} _{z_{i}}
\]

\end_inset

 The represetations of 
\begin_inset Formula $G$
\end_inset

 transform as
\begin_inset Formula 
\[
Z\to Z^{\prime}=U_{Z}Z,\quad U_{Z}=\bigoplus_{i\,\mathrm{in}\, Z}u_{i},
\]

\end_inset

 similarly for 
\begin_inset Formula $X$
\end_inset

 and 
\begin_inset Formula $Y$
\end_inset

.
 Consider 
\begin_inset Formula 
\begin{align*}
W^{\prime} & =\bigoplus_{i}\sum_{j,k}c_{i,jk}\left\{ \left(P_{X\to j}X^{\prime}\right)\otimes\left(P_{Y\to k}Y^{\prime}\right)\right\} _{z_{i}^{\prime}}^{\prime}\\
 & =\bigoplus_{i}\sum_{j,k}c_{i,jk}\left\{ \left(P_{X\to j}\left(\bigoplus_{a\,\mathrm{in}\, X}u_{a}\right)X\right)\otimes\left(P_{Y\to k}\left(\bigoplus_{b\,\mathrm{in}\, Y}u_{b}\right)Y\right)\right\} _{z_{i}^{\prime}}^{\prime}\\
 & =\bigoplus_{i}\sum_{j,k}c_{i,jk}\left\{ \left(u_{j}P_{X\to j}X\right)\otimes\left(u_{k}P_{Y\to k}Y\right)\right\} _{z_{i}^{\prime}}^{\prime}\\
 & =\bigoplus_{i}\sum_{j,k}c_{i,jk}u_{i}\left\{ \left(P_{X\to j}X\right)\otimes\left(P_{Y\to k}Y\right)\right\} _{z_{i}}\\
 & =\left(\bigoplus_{i}u_{i}\right)\sum_{j,k}c_{i,jk}\left\{ \left(P_{X\to j}X\right)\otimes\left(P_{Y\to k}Y\right)\right\} _{z_{i}}\\
 & =U_{Z}Z=Z^{\prime}
\end{align*}

\end_inset

Under the transform of any 
\begin_inset Formula $g\in G$
\end_inset

, 
\begin_inset Formula 
\[
R_{Z}^{\prime}\left(g\right)=U_{Z}R_{Z}\left(g\right)U_{Z}^{-1}.
\]

\end_inset

 
\begin_inset Formula 
\begin{align*}
Z^{\prime}\to Z^{\prime\prime} & =U_{Z}R_{Z}\left(g\right)Z\\
 & =U_{Z}R_{Z}\left(g\right)U_{Z}^{-1}U_{Z}Z\\
 & =R_{Z}^{\prime}Z^{\prime}.
\end{align*}

\end_inset

 
\end_layout

