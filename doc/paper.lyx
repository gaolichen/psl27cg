\begin_layout Standard
The non-singlet irreps of 
\begin_inset Formula $S_{4}$
\end_inset

 can be expressed as 
\begin_inset Formula 
\begin{equation}
\rep{2}:\quad a=\begin{pmatrix}0 & 1\\
1 & 0
\end{pmatrix},\quad b=\left(\begin{array}{cc}
0 & \omega^{2}\\
\omega & 0
\end{array}\right),\label{eq:S4_irr2}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{align}
\rep{3_{1}}:\quad a^{[3_{1}]} & =\frac{1}{3}\left(\begin{array}{ccc}
-1 & 2\omega^{2} & 2\omega\\
2\omega^{2} & 2\omega & -1\\
2\omega & -1 & 2\omega^{2}
\end{array}\right),\quad b^{[3_{1}]}=\frac{1}{3}\left(\begin{array}{ccc}
-1 & 2\omega^{2} & 2\omega\\
2\omega & 2 & -\omega^{2}\\
2\omega^{2} & -\omega & 2
\end{array}\right),\label{eq:S4_irr31}\\
a^{[3_{2}]} & =-a^{[3_{1}]},\quad b^{[3_{2}]}=-b^{[3_{1}]},
\end{align}

\end_inset

 where, in the 
\begin_inset Formula $\rep{2}$
\end_inset

 representation 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:S4_irr2"

\end_inset

, 
\begin_inset Formula $A_{4}$
\end_inset

 elements are diagonal matrices, and in the triplet representations, 
\begin_inset Formula $A_{4}$
\end_inset

 elements are generated by 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:A4-generators"

\end_inset

.
\end_layout

\begin_layout Standard
Using the CG coefficients of 
\begin_inset Formula $A_{4}$
\end_inset

, we obtain the CG coefficients of two triplets of 
\begin_inset Formula $S_{4}$
\end_inset

.
 In the following CG coefficients, 
\begin_inset Formula $ $
\end_inset


\begin_inset Formula $\rep{x}$
\end_inset

 and 
\begin_inset Formula $\rep{y}$
\end_inset

 are triplets, 
\begin_inset Formula $\rep{z}=\left(\rep{s_{z}^{\prime}},\rep{\bar{s}_{z}^{\prime}}\right)$
\end_inset

 and 
\begin_inset Formula $\rep{w}=\left(\rep{s_{w}^{\prime}},\rep{\bar{s}_{w}^{\prime}}\right)$
\end_inset

 are doublets, where 
\begin_inset Formula $s^{\prime}$
\end_inset

 and 
\begin_inset Formula $\bar{s}^{\prime}$
\end_inset

 are nontrivial singlets of 
\begin_inset Formula $A_{4}$
\end_inset

, and 
\begin_inset Formula $\rep{s}$
\end_inset

 is a singlet of 
\begin_inset Formula $S_{4}$
\end_inset

 and 
\begin_inset Formula $A_{4}$
\end_inset

.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{3_{1}}\otimes\rep{3_{1}}\to\left(\rep{1}+\rep{2}+\rep{3_{1}}\right)_{s}+\rep{3_{2}}_{a}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\covs{x}{y}{1} & =\cova{x}{y}{1}\\
\covs{x}{y}{2} & =\begin{pmatrix}\cova{x}{y}{1^{\prime}}\\
\cova{x}{y}{\bar{1}^{\prime}}
\end{pmatrix}\\
\covs{x}{y}{3_{1}} & =\cova{x}{y}{3_{s}}\\
\covs{x}{y}{3_{2}} & =i\cova{x}{y}{3_{a}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{3_{2}}\otimes\rep{3_{2}}\to\left(\rep{1}+\rep{2}+\rep{3_{1}}\right)_{s}+\rep{3_{2}}_{a}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\covs{x}{y}{1} & =\cova{x}{y}{1}\\
\covs{x}{y}{2} & =\begin{pmatrix}\cova{x}{y}{1^{\prime}}\\
\cova{x}{y}{\bar{1}^{\prime}}
\end{pmatrix}\\
\covs{x}{y}{3_{1}} & =\cova{x}{y}{3_{s}}\\
\covs{x}{y}{3_{2}} & =i\cova{x}{y}{3_{a}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{3_{1}}\otimes\rep{3_{2}}\to\rep{1_{1}}+\rep{2}+\rep{3_{1}}+\rep{3_{2}}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\left[\rep{x}\otimes\rep{y}\right]_{\rep{1_{1}}} & =\left(\rep{x}\otimes\rep{y}\right)_{\rep{1}}\\
\left[\rep{x}\otimes\rep{y}\right]_{\rep{2}} & =\begin{pmatrix}i\left(\rep{x}\otimes\rep{y}\right)_{\rep{1^{\prime}}}\\
-i\left(\rep{x}\otimes\rep{y}\right)_{\rep{\bar{1}^{\prime}}}
\end{pmatrix}\\
\left[\rep{x}\otimes\rep{y}\right]_{\rep{3_{1}}} & =i\left(\rep{x}\otimes\rep{y}\right)_{\rep{3_{a}}}\\
\left[\rep{x}\otimes\rep{y}\right]_{\rep{3_{2}}} & =\left(\rep{x}\otimes\rep{y}\right)_{\rep{3_{s}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{3_{1}}\otimes\rep{2}\to\rep{3_{1}}+\rep{3_{2}}$
\end_inset

: 
\begin_inset Formula 
\begin{align*}
\left[\rep{x}\otimes\rep{z}\right]_{\rep{3_{1}}} & =\frac{1}{\sqrt{2}}\left(\rep{x}\otimes\rep{s_{z}^{\prime}}\right)_{\rep{3}}+\frac{1}{\sqrt{2}}\left(\rep{x}\otimes\rep{\bar{s}_{z}^{\prime}}\right)_{\rep{3}}\\
\left[\rep{x}\otimes\rep{z}\right]_{\rep{3_{2}}} & =\frac{i}{\sqrt{2}}\left(\rep{x}\otimes\rep{s_{z}^{\prime}}\right)_{\rep{3}}-\frac{i}{\sqrt{2}}\left(\rep{x}\otimes\rep{\bar{s}_{z}^{\prime}}\right)_{\rep{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{3_{2}}\otimes\rep{2}\to\rep{3_{1}}+\rep{3_{2}}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\left[\rep{x}\otimes\rep{z}\right]_{\rep{3_{1}}} & =\frac{i}{\sqrt{2}}\left(\rep{x}\otimes\rep{s_{z}^{\prime}}\right)_{\rep{3}}-\frac{i}{\sqrt{2}}\left(\rep{x}\otimes\rep{\bar{s}_{z}^{\prime}}\right)_{\rep{3}}\\
\left[\rep{x}\otimes\rep{z}\right]_{\rep{3_{2}}} & =\frac{1}{\sqrt{2}}\left(\rep{x}\otimes\rep{s_{z}^{\prime}}\right)_{\rep{3}}+\frac{1}{\sqrt{2}}\left(\rep{x}\otimes\rep{\bar{s}_{z}^{\prime}}\right)_{\rep{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{2}\otimes\rep{2}\to\left(\rep{1}+\rep{2}\right)_{s}+\rep{1_{1}}_{a}$
\end_inset

: 
\begin_inset Formula 
\begin{align*}
\left[\rep{z}\otimes\rep{w}\right]_{\rep{1}} & =\frac{1}{\sqrt{2}}\left(\rep{s_{z}^{\prime}}\otimes\rep{\bar{s}_{w}^{\prime}}\right)_{\rep{1}}+\frac{1}{\sqrt{2}}\left(\rep{\bar{s}_{z}^{\prime}}\otimes\rep{s_{w}^{\prime}}\right)_{\rep{1}}\\
\left[\rep{z}\otimes\rep{w}\right]_{\rep{1_{1}}} & =\frac{i}{\sqrt{2}}\left(\rep{s_{z}^{\prime}}\otimes\rep{\bar{s}_{w}^{\prime}}\right)_{\rep{1}}-\frac{i}{\sqrt{2}}\left(\rep{\bar{s}_{z}^{\prime}}\otimes\rep{s_{w}^{\prime}}\right)_{\rep{1}}\\
\left[\rep{z}\otimes\rep{w}\right]_{\rep{2}} & =\begin{pmatrix}\left(\rep{\bar{s}_{z}^{\prime}}\otimes\rep{\bar{s}_{w}^{\prime}}\right)_{\rep{1^{\prime}}}\\
\left(\rep{s_{z}^{\prime}}\otimes\rep{s_{w}^{\prime}}\right)_{\rep{\bar{1}^{\prime}}}
\end{pmatrix}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\rep{2}\otimes\rep{1_{1}}\to\rep{2}$
\end_inset

:
\begin_inset Formula 
\[
\left[\rep{z}\otimes\rep{s}\right]_{\rep{2}}=\begin{pmatrix}i\left(\rep{s_{z}^{\prime}}\otimes\rep{s}\right)_{\rep{1^{\prime}}}\\
-i\left(\rep{\bar{s}_{z}^{\prime}}\otimes\rep{s}\right)_{\rep{\bar{1}^{\prime}}}
\end{pmatrix}
\]

\end_inset


\end_layout

\begin_layout Subsection
\begin_inset Formula $\subgb$
\end_inset


\end_layout

\begin_layout Standard
The Frobenius group of order 21 is the smallest finite non-Abelian subgroup
 of 
\begin_inset Formula $SU\left(3\right)$
\end_inset

.
 It contains elements of order three and seven, with the presentation 
\begin_inset Formula 
\begin{equation}
\left\langle c,d\big|c^{7}=d^{3}=1,d^{-1}cd=c^{4}\right\rangle .\label{eq:Z7Z3-Generator}
\end{equation}

\end_inset

Its irreps are, a real singlet, one complex triplet 
\begin_inset Formula $\rep{3}$
\end_inset

, a complex singlet, 
\begin_inset Formula $\rep{1^{\prime}}$
\end_inset

, and their inequivalent conjugates, 
\begin_inset Formula $\rep{\bar{3}}$
\end_inset

, and 
\begin_inset Formula $\rep{\bar{1}^{\prime}}$
\end_inset

.
 Their Kronecker products are 
\begin_inset Formula 
\begin{align*}
\rep{1^{\prime}}\otimes\rep{1^{\prime}} & =\rep{\bar{1}^{\prime}},\quad\quad\quad\quad\quad\;\,\rep{1^{\prime}}\otimes\rep{\bar{1}^{\prime}}=\rep{1}\\
\rep{3}\otimes\rep{1^{\prime}} & =\rep{3},\quad\quad\quad\quad\quad\quad\rep{3}\otimes\rep{\bar{1}^{\prime}}=\rep{3}\\
\rep{3}\otimes\rep{3}\; & =\left(\rep{3}+\rep{\bar{3}}\right)_{s}+\rep{\bar{3}}_{a},\;\;\,\rep{3}\otimes\rep{\bar{3}}\;=\rep{1}+\rep{1^{\prime}}+\rep{\bar{1}^{\prime}}+\rep{3}+\rep{\bar{3}}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The CG coefficients of 
\begin_inset Formula $\subgb$
\end_inset

 are as follows.
\begin_inset Formula 
\begin{align*}
\covt{1^{\prime}}{\bar{3}}{3} & =\begin{pmatrix}\ket{1^{\prime}}\ket{1}\\
\omega\ket{1^{\prime}}\ket{2}\\
\omega^{2}\ket{1^{\prime}}\ket{3}
\end{pmatrix},\quad\quad\quad\quad\quad\quad\covt{3}{3}{3}=\begin{pmatrix}\ket{3}\ket{3^{\prime}}\\
\ket{1}\ket{1^{\prime}}\\
\ket{2}\ket{2^{\prime}}
\end{pmatrix}\\
\covt{3}{3}{\bar{3}_{s}} & =\begin{pmatrix}\frac{1}{\sqrt{2}}\left(\ket{3}\ket{2^{\prime}}+\ket{2}\ket{3^{\prime}}\right)\\
\frac{1}{\sqrt{2}}\left(\ket{1}\ket{3^{\prime}}+\ket{3}\ket{1^{\prime}}\right)\\
\frac{1}{\sqrt{2}}\left(\ket{2}\ket{1^{\prime}}+\ket{1}\ket{2^{\prime}}\right)
\end{pmatrix},\quad\covt{3}{3}{\bar{3}_{s}}=\begin{pmatrix}\frac{1}{\sqrt{2}}\left(\ket{3}\ket{2^{\prime}}-\ket{2}\ket{3^{\prime}}\right)\\
\frac{1}{\sqrt{2}}\left(\ket{1}\ket{3^{\prime}}-\ket{3}\ket{1^{\prime}}\right)\\
\frac{1}{\sqrt{2}}\left(\ket{2}\ket{1^{\prime}}-\ket{1}\ket{2^{\prime}}\right)
\end{pmatrix}\\
\covt{3}{\bar{3}}{3} & =\begin{pmatrix}\ket{2}\ket{\bar{1}}\\
\ket{3}\ket{\bar{2}}\\
\ket{1}\ket{\bar{3}}
\end{pmatrix},\quad\quad\quad\quad\quad\quad\quad\quad\covt{3}{\bar{3}}{\bar{3}}=\begin{pmatrix}\ket{1}\ket{\bar{2}}\\
\ket{2}\ket{\bar{3}}\\
\ket{3}\ket{\bar{1}}
\end{pmatrix}\\
\covt{3}{\bar{3}}{1^{\prime}} & =\frac{1}{\sqrt{3}}\left(\ket{1}\ket{\bar{1}}+\omega^{2}\ket{2}\ket{\bar{2}}+\omega\ket{3}\ket{\bar{3}}\right),\quad\omega=\exp\left(\frac{2i\pi}{3}\right)\\
\covt{3}{\bar{3}}{1^{\prime}} & =\frac{1}{\sqrt{3}}\left(\ket{1}\ket{\bar{1}}+\omega\ket{2}\ket{\bar{2}}+\omega^{2}\ket{3}\ket{\bar{3}}\right)
\end{align*}

\end_inset


\end_layout

