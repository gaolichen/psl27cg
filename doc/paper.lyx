\begin{equation}
e^{i\theta_{2}}=-i\bar{b}_{7}^{2}e^{i\theta_{1}},\quad e^{i\theta_{3}}=i\bar{b}_{7}e^{i\theta_{1}}.\label{eq:Gen_336Phases}
\end{equation}

