\begin_layout Standard
The notations of the this section are as follows.
 For each Kronecker product 
\begin_inset Formula $\repx{A}{x}\otimes\repx{B}{y}\to\rep{C}$
\end_inset

, we use the left subscripts 
\begin_inset Formula $x$
\end_inset

 and 
\begin_inset Formula $y$
\end_inset

 to label the two 
\begin_inset Formula $\mathcal{PSL}_{2}\left(7\right)$
\end_inset

 representations on LHS.
 In 
\begin_inset Formula $S_{4}$
\end_inset

 basis, the representation 
\begin_inset Formula $\rep{C}=\rep{c_{1}}\oplus\rep{c_{2}}\oplus\cdots$
\end_inset

 can be written as 
\begin_inset Formula 
\[
\rep{c_{k}}=\sum_{i,j}g_{ijk}\subcg{a_{i}}{b_{j}}{c_{k}},
\]

\end_inset

 where 
\begin_inset Formula $g_{ijk}$
\end_inset

 are CG coefficients, 
\begin_inset Formula $\rep{a_{i}}$
\end_inset

 and 
\begin_inset Formula $\rep{b_{j}}$
\end_inset

 are representations of 
\begin_inset Formula $S_{4}$
\end_inset

 group, and the labels 
\begin_inset Formula $x$
\end_inset

 and 
\begin_inset Formula $y$
\end_inset

 are used to indicate the embedding relations 
\begin_inset Formula $\rep{A}=\rep{a_{1}}\oplus\rep{a_{2}}\oplus\cdots$
\end_inset

 and 
\begin_inset Formula $\rep{B}=\rep{b_{1}}\oplus\rep{b_{2}}\oplus\cdots$
\end_inset

.
 For convenience, we define 
\begin_inset Formula 
\[
b_{7}=\frac{1}{\sqrt{2}}\left(\eta+\eta^{2}+\eta^{4}\right)=\frac{1+i\sqrt{7}}{2\sqrt{2}},\quad\eta^{7}=1.
\]

\end_inset


\end_layout

\begin_layout Paragraph*

\size larger
\begin_inset Formula $\rep{3}\otimes\rep{3}\to\rep{6}_{s}+\rep{\bar{3}}_{a}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{3}{y}\to\rep{\bar{3}}_{a}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{3}_{2} & = & \subcg{3_{2}}{3_{2}}{3_{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{3}{y}\to\rep{6}_{s}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{1}_{0} & = & \subcg{3_{2}}{3_{2}}{1_{0}}\\
\rep{2} & = & -\frac{i}{b_{7}^{2}}\subcg{3_{2}}{3_{2}}{2}\\
\rep{3}_{1} & = & \frac{i}{b_{7}}\subcg{3_{2}}{3_{2}}{3_{1}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Paragraph*

\size larger
\begin_inset Formula $\rep{3}\otimes\rep{\bar{3}}\to\rep{1}+\rep{8}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{\bar{3}}{y}\to\rep{1}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{1}_{0} & = & \subcg{3_{2}}{3_{2}}{1_{0}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{\bar{3}}{y}\to\rep{8}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{2} & = & \subcg{3_{2}}{3_{2}}{2}\\
\rep{3}_{1} & = & \subcg{3_{2}}{3_{2}}{3_{1}}\\
\rep{3}_{2} & = & i\subcg{3_{2}}{3_{2}}{3_{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Paragraph*

\size larger
\begin_inset Formula $\rep{3}\otimes\rep{6}\to\rep{\bar{3}}+\rep{7}+\rep{8}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{6}{y}\to\rep{\bar{3}}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{3}_{2} & = & \frac{1}{\sqrt{6}}\subcg{3_{2}}{1_{0}}{3_{2}}-\frac{i}{\sqrt{3}b_{7}^{2}}\subcg{3_{2}}{2}{3_{2}}+\frac{i}{\sqrt{2}b_{7}}\subcg{3_{2}}{3_{1}}{3_{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{6}{y}\to\rep{7}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{1}_{1} & = & \subcg{3_{2}}{3_{1}}{1_{1}}\\
\rep{3}_{1} & = & \frac{1}{\sqrt{3}b_{7}^{3}}\subcg{3_{2}}{2}{3_{1}}+\frac{\sqrt{\frac{2}{3}}}{b_{7}^{4}}\subcg{3_{2}}{3_{1}}{3_{1}}\\
\rep{3}_{2} & = & \frac{\sqrt{\frac{7}{2}}}{3b_{7}}\subcg{3_{2}}{1_{0}}{3_{2}}+\frac{2}{3b_{7}^{5}}\subcg{3_{2}}{2}{3_{2}}-\frac{1}{\sqrt{6}b_{7}^{2}}\subcg{3_{2}}{3_{1}}{3_{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{6}{y}\to\rep{8}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{2} & = & \subcg{3_{2}}{3_{1}}{2}\\
\rep{3}_{1} & = & -\sqrt{\frac{2}{3}}b_{7}\subcg{3_{2}}{2}{3_{1}}+\frac{1}{\sqrt{3}}\subcg{3_{2}}{3_{1}}{3_{1}}\\
\rep{3}_{2} & = & \frac{2}{3b_{7}}\subcg{3_{2}}{1_{0}}{3_{2}}-\frac{1}{3}i\sqrt{2}b_{7}\subcg{3_{2}}{2}{3_{2}}+\frac{i}{\sqrt{3}}\subcg{3_{2}}{3_{1}}{3_{2}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Paragraph*

\size larger
\begin_inset Formula $\rep{3}\otimes\rep{7}\to\rep{6}+\rep{7}+\rep{8}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{7}{y}\to\rep{6}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{1}_{0} & = & \subcg{3_{2}}{3_{2}}{1_{0}}\\
\rep{2} & = & -\frac{\sqrt{\frac{3}{7}}}{b_{7}^{2}}\subcg{3_{2}}{3_{1}}{2}+\frac{2}{\sqrt{7}b_{7}^{4}}\subcg{3_{2}}{3_{2}}{2}\\
\rep{3}_{1} & = & \sqrt{\frac{2}{7}}b_{7}\subcg{3_{2}}{1_{1}}{3_{1}}-\frac{2}{\sqrt{7}b_{7}^{3}}\subcg{3_{2}}{3_{1}}{3_{1}}-\frac{1}{\sqrt{7}b_{7}}\subcg{3_{2}}{3_{2}}{3_{1}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\repx{3}{x}\otimes\repx{7}{y}\to\rep{7}$
\end_inset

: 
\begin_inset Formula 
\begin{eqnarray*}
\rep{1}_{1} & = & \subcg{3_{2}}{3_{1}}{1_{1}}\\
\rep{3}_{1} & = & -\frac{1}{\sqrt{3}}\subcg{3_{2}}{1_{1}}{3_{1}}-\frac{1}{\sqrt{3}b_{7}}\subcg{3_{2}}{3_{1}}{3_{1}}-\frac{1}{\sqrt{3}b_{7}}\subcg{3_{2}}{3_{2}}{3_{1}}\\
\rep{3}_{2} & = & \frac{1}{\sqrt{3}b_{7}}\subcg{3_{2}}{3_{1}}{3_{2}}+\frac{\sqrt{\frac{2}{3}}}{b_{7}^{2}}\subcg{3_{2}}{3_{2}}{3_{2}}
