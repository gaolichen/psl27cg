\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\tr ABABB & =\tr UAU^{-1}\hat{B}UAU^{-1}\hat{B}^{2}\\
 & =\tr UAU^{-1}\hat{B}UAU^{-1}\left(\frac{2y^{2}+1}{3}+\frac{y^{2}-1}{\sqrt{3}}\lambda_{8}\right)\\
 & =\frac{2y^{2}+1}{3}\tr UAU^{-1}\hat{B}UAU^{-1}+\frac{y^{2}-1}{\sqrt{3}}\tr UAU^{-1}\hat{B}UAU^{-1}\lambda_{8}\\
 & =\frac{2y^{2}+1}{3}\tr UAU^{-1}UAU^{-1}\hat{B}+\frac{y^{2}-1}{\sqrt{3}}\tr UAU^{-1}\lambda_{8}UAU^{-1}\hat{B}\\
 & =\frac{2y^{2}+1}{3}\tr UA^{2}U^{-1}\hat{B}+\frac{y^{2}-1}{\sqrt{3}}\tr UAU^{-1}\lambda_{8}UAU^{-1}\hat{B}\\
 & \equiv\frac{2y^{2}+1}{3}\tr X\hat{B}+\frac{y^{2}-1}{\sqrt{3}}\tr Y\hat{B}
\end{align*}

\end_inset

 where we have defined 
\begin_inset Formula $X$
\end_inset

 and 
\begin_inset Formula $Y$
\end_inset

:
\begin_inset Formula 
\[
X=UA^{2}U^{-1},\quad Y=UAU^{-1}\lambda_{8}UAU^{-1}.
\]

\end_inset

 Now 
\begin_inset Formula 
\begin{align*}
X & =UA^{2}U^{-1}\\
 & =U\left(\frac{2x^{2}+1}{3}+\frac{x^{2}-1}{\sqrt{3}}\lambda_{8}\right)U^{-1}\\
 & =\frac{2x^{2}+1}{3}+\frac{x^{2}-1}{\sqrt{3}}Z_{8i}\lambda_{i}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tr X\hat{B} & =\tr\left(\frac{2x^{2}+1}{3}+\frac{x^{2}-1}{\sqrt{3}}Z_{8i}\lambda_{i}\right)\left(\frac{1}{3}+y\lambda_{3}-\frac{1}{\sqrt{3}}\lambda_{8}\right)\\
 & =\frac{2x^{2}+1}{3}+\frac{x^{2}-1}{\sqrt{3}}Z_{8i}\left(2y\delta_{3i}-\frac{2}{\sqrt{3}}\delta_{8i}\right)\\
 & =\frac{2x^{2}+1}{3}+\frac{2\left(x^{2}-1\right)}{\sqrt{3}}\left(yZ_{83}-\frac{Z_{88}}{\sqrt{3}}\right).
\end{align*}

\end_inset

 To calculate 
\begin_inset Formula $\tr Y\hat{B}$
\end_inset

:
\begin_inset Formula 
\begin{align*}
\tr Y\hat{B} & =\tr Y\left(\frac{1}{3}+y\lambda_{3}-\frac{1}{\sqrt{3}}\lambda_{8}\right)\\
 & =\frac{1}{3}\tr Y+y\tr Y\lambda_{3}-\frac{1}{\sqrt{3}}\tr Y\lambda_{8}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tr Y & =\tr UAU^{-1}\lambda_{8}UAU^{-1}\\
 & =\tr UA^{2}U^{-1}\lambda_{8}\\
 & =\tr\left(\frac{2x^{2}+1}{3}+\frac{x^{2}-1}{\sqrt{3}}Z_{8i}\lambda_{i}\right)\lambda_{8}\\
 & =\frac{2\left(x^{2}-1\right)}{\sqrt{3}}Z_{88}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
For trace of 
\begin_inset Formula $Y$
\end_inset

 with 
\begin_inset Formula $\lambda_{3}$
\end_inset

 and 
\begin_inset Formula $\lambda_{8}$
\end_inset

, we need some identities: 
\begin_inset Formula 
\[
\frac{\lambda_{i}}{2}\frac{\lambda_{j}}{2}=\frac{1}{2}\left[\frac{1}{3}\delta_{ij}+\left(d_{ijk}+if_{ijk}\right)\frac{\lambda_{k}}{2}\right],\quad\left\{ \lambda_{i},\lambda_{j}\right\} =\frac{4}{3}\delta_{ij}+2d_{ijk}\lambda_{k},\quad\tr\frac{\lambda_{a}}{2}\frac{\lambda_{b}}{2}\frac{\lambda_{c}}{2}=\frac{1}{4}\left(d_{abc}+if_{abc}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
Y & =UAU^{-1}\lambda_{8}UAU^{-1}\\
 & =U\left(\frac{1}{3}+x\lambda_{3}-\frac{1}{\sqrt{3}}\lambda_{8}\right)U^{-1}\lambda_{8}U\left(\frac{1}{3}+x\lambda_{3}-\frac{1}{\sqrt{3}}\lambda_{8}\right)U^{-1}\\
 & =\left(\frac{1}{3}+x\sum_{i}Z_{3i}\lambda_{i}-\frac{1}{\sqrt{3}}\sum_{i}Z_{8i}\lambda_{i}\right)\lambda_{8}\left(\frac{1}{3}+x\sum_{i}Z_{3i}\lambda_{i}-\frac{1}{\sqrt{3}}\sum_{i}Z_{8i}\lambda_{i}\right)\\
 & =\frac{\lambda_{8}}{9}+\frac{1}{3}\sum_{i}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left\{ \lambda_{i},\lambda_{8}\right\} +\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\lambda_{i}\lambda_{8}\lambda_{j}\\
 & =\frac{\lambda_{8}}{9}+\frac{1}{3}\sum_{i}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(\frac{4}{3}\delta_{i8}+2d_{i8k}\lambda_{k}\right)\\
 & +\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\lambda_{i}\lambda_{8}\lambda_{j}\\
 & =\frac{\lambda_{8}}{9}+\frac{4}{9}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)+\frac{2}{3}\sum_{i,k}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)d_{i8k}\lambda_{k}\\
 & +\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\left(\lambda_{i}\lambda_{8}\lambda_{j}\right)\\
 & \equiv Y_{1}+Y_{2}.
\end{align*}

\end_inset


\begin_inset Formula 
\[
Y_{1}=\frac{\lambda_{8}}{9}+\frac{4}{9}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)+\frac{2}{3}\sum_{i,k}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)d_{i8k}\lambda_{k}
\]

\end_inset


\begin_inset Formula 
\[
Y_{2}=\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\left(\lambda_{i}\lambda_{8}\lambda_{j}\right)
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\tr Y\lambda_{3} & =\tr\left(Y_{1}+Y_{2}\right)\lambda_{3}\\
 & =\frac{4}{3}\sum_{i}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)d_{i83}+\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\tr\left(\lambda_{i}\lambda_{8}\lambda_{j}\lambda_{3}\right)\\
 & =\frac{4}{3\sqrt{3}}\left(xZ_{33}-\frac{Z_{83}}{\sqrt{3}}\right)\\
 & +\frac{1}{2}\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\tr\left(\lambda_{i}\lambda_{8}\lambda_{j}\lambda_{3}+\lambda_{j}\lambda_{8}\lambda_{i}\lambda_{3}\right)\\
 & =\frac{4}{3\sqrt{3}}\left(xZ_{33}-\frac{Z_{83}}{\sqrt{3}}\right)+2\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(\Sigma-\frac{2}{\sqrt{3}}D^{3}\right)_{ij}\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\\
 & =\frac{4}{3\sqrt{3}}\left(xZ_{33}-\frac{Z_{83}}{\sqrt{3}}\right)+2\mathcal{F}\left(\Sigma-\frac{2}{\sqrt{3}}D^{3}\right)
\end{align*}

\end_inset

Where we defined 
\begin_inset Formula 
\begin{align*}
\mathcal{F}\left(M\right) & \equiv\sum_{i,j}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)M_{ij}\left(xZ_{j3}^{T}-\frac{Z_{j8}^{T}}{\sqrt{3}}\right)\\
 & =x^{2}\left(ZMZ^{T}\right)_{33}-\frac{x}{\sqrt{3}}\left[\left(ZMZ^{T}\right)_{38}+\left(ZMZ^{T}\right)_{83}\right]+\frac{1}{3}\left(ZMZ^{T}\right)_{88}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tr Y\lambda_{8} & =\tr\left(Y_{1}+Y_{2}\right)\lambda_{8}\\
 & =\frac{2}{9}+\frac{4}{3}\sum_{i}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)d_{i88}+\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\tr\left(\lambda_{i}\lambda_{8}\lambda_{j}\lambda_{8}\right)\\
 & =\frac{2}{9}+\frac{4}{3}\sum_{i}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)d_{i88}\\
 & +\frac{1}{2}\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\tr\left(\lambda_{i}\lambda_{8}\lambda_{j}\lambda_{8}+\lambda_{j}\lambda_{8}\lambda_{i}\lambda_{8}\right)\\
 & =\frac{2}{9}-\frac{4}{3\sqrt{3}}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)\\
 & +\sum_{ij}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)\left(xZ_{3j}-\frac{Z_{8j}}{\sqrt{3}}\right)\left[4\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)_{ij}-\frac{2}{3}\delta_{ij}\right]\\
 & =\frac{2}{9}-\frac{4}{3\sqrt{3}}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)-\frac{2}{3}\sum_{i}\left(xZ_{3i}-\frac{Z_{8i}}{\sqrt{3}}\right)^{2}\\
 & +4\mathcal{F}\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)\\
 & =\frac{2}{9}-\frac{4}{3\sqrt{3}}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)-\frac{2}{3}\left(x^{2}+\frac{1}{3}\right)+4\mathcal{F}\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)\\
 & =-\frac{2}{3}x^{2}-\frac{4}{3\sqrt{3}}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)+4\mathcal{F}\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tr Y\hat{B} & =\tr Y\left(\frac{1}{3}+y\lambda_{3}-\frac{1}{\sqrt{3}}\lambda_{8}\right)\\
 & =\frac{1}{3}\tr Y+y\tr Y\lambda_{3}-\frac{1}{\sqrt{3}}\tr Y\lambda_{8}\\
 & =\frac{2\left(x^{2}-1\right)}{3\sqrt{3}}Z_{88}+y\left[\frac{4}{3\sqrt{3}}\left(xZ_{33}-\frac{Z_{83}}{\sqrt{3}}\right)+2\mathcal{F}\left(\Sigma-\frac{2}{\sqrt{3}}D^{3}\right)\right]\\
 & -\frac{1}{\sqrt{3}}\left[-\frac{2}{3}x^{2}-\frac{4}{3\sqrt{3}}\left(xZ_{38}-\frac{Z_{88}}{\sqrt{3}}\right)+4\mathcal{F}\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)\right]\\
 & =\frac{2x^{2}}{3\sqrt{3}}+\frac{4xy}{3\sqrt{3}}Z_{33}+\frac{4}{9}\left(xZ_{38}-yZ_{83}\right)+\frac{6x^{2}-10}{9\sqrt{3}}Z_{88}\\
 & +2y\mathcal{F}\left(\Sigma-\frac{2}{\sqrt{3}}D^{3}\right)-\frac{4}{\sqrt{3}}\mathcal{F}\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tr ABABB & =\frac{2y^{2}+1}{3}\tr X\hat{B}+\frac{y^{2}-1}{\sqrt{3}}\tr Y\hat{B}\\
 & =\frac{2y^{2}+1}{3}\left[\frac{2x^{2}+1}{3}+\frac{2\left(x^{2}-1\right)}{\sqrt{3}}\left(yZ_{83}-\frac{Z_{88}}{\sqrt{3}}\right)\right]\\
 & +\frac{y^{2}-1}{\sqrt{3}}\left[\frac{2x^{2}}{3\sqrt{3}}+\frac{4xy}{3\sqrt{3}}Z_{33}+\frac{4}{9}\left(xZ_{38}-yZ_{83}\right)+\frac{6x^{2}-10}{9\sqrt{3}}Z_{88}\right]\\
 & +\frac{y^{2}-1}{\sqrt{3}}\left[2y\mathcal{F}\left(\Sigma-\frac{2}{\sqrt{3}}D^{3}\right)-\frac{4}{\sqrt{3}}\mathcal{F}\left(\Omega+\frac{1}{\sqrt{3}}D^{8}\right)\right]\\
