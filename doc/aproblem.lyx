\begin_layout Standard
\begin_inset Formula 
\[
A=\begin{pmatrix}x\\
 & -x\\
 &  & 1
\end{pmatrix},\quad B=U^{-1}\hat{B}U\equiv U^{-1}\begin{pmatrix}y\\
 & -y\\
 &  & 1
\end{pmatrix}U,\quad U=\exp\left(ia_{k}\frac{\lambda_{k}}{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\lambda_{3}=\begin{pmatrix}1\\
 & -1\\
\\
\end{pmatrix},\quad\lambda_{8}=\frac{1}{\sqrt{3}}\begin{pmatrix}1\\
 & 1\\
 &  & -2
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
U\frac{\lambda_{n}}{2}U^{-1} & =\exp\left(ia_{k}\frac{\lambda_{k}}{2}\right)\frac{\lambda_{n}}{2}\exp\left(-ia_{k}\frac{\lambda_{k}}{2}\right)\\
 & =\frac{\lambda_{n}}{2}+\left[ia_{i}\frac{\lambda_{i}}{2},\frac{\lambda_{n}}{2}\right]+\frac{1}{2!}\left[ia_{j}\frac{\lambda_{j}}{2},\left[ia_{i}\frac{\lambda_{i}}{2},\frac{\lambda_{n}}{2}\right]\right]+\frac{1}{3!}\left[ia_{k}\frac{\lambda_{k}}{2},\left[ia_{j}\frac{\lambda_{j}}{2},\left[ia_{i}\frac{\lambda_{i}}{2},\frac{\lambda_{n}}{2}\right]\right]\right]+\cdots\\
 & =\frac{\lambda_{n}}{2}+\sum_{i}i^{2}a_{i}f_{inm}\frac{\lambda_{m}}{2}+\frac{1}{2!}\sum_{i,j}i^{4}a_{i}a_{j}f_{inm}f_{jml}\frac{\lambda_{l}}{2}+\frac{1}{3!}\sum_{i,j,k}i^{6}a_{i}a_{j}a_{k}f_{inm}f_{jml}f_{kmn}\frac{\lambda_{n}}{2}+\cdots\\
 & =\frac{\lambda_{n}}{2}+\left(\sum_{i}-a_{i}f_{i}\right)_{nm}\frac{\lambda_{m}}{2}+\frac{1}{2!}\left[\left(\sum_{i}-a_{i}f_{i}\right)^{2}\right]_{nm}\frac{\lambda_{m}}{2}+\frac{1}{3!}\left[\left(\sum_{i}-a_{i}f_{i}\right)^{3}\right]_{nm}\frac{\lambda_{m}}{2}+\cdots\\
 & =\left[\exp\left(\sum_{i}-a_{i}f_{i}\right)\right]_{nm}\frac{\lambda_{m}}{2}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\tr U\frac{\lambda_{n}}{2}U^{-1}\lambda_{m} & =\frac{1}{2}\left[\exp\left(\sum_{i}-a_{i}f_{i}\right)\right]_{nl}\tr\lambda_{l}\lambda_{m}\\
 & =\left[\exp\left(\sum_{i}-a_{i}f_{i}\right)\right]_{nl}\delta_{lm}\\
 & =\left[\exp\left(\sum_{i}-a_{i}f_{i}\right)\right]_{nm}\\
 & \equiv Z_{nm}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
